<?xml version="1.0" encoding="utf-8" ?>
<TestSite xmlns="http://schemas.microsoft.com/windows/ProtocolsTest/2007/07/TestConfig"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!-- Note: Property values are case-insensitive unless otherwise specified. -->
  <Properties>
    <!-- Set R318Enabled to true to verify that the implementation uses secure communications via HTTPS, defined in [RFC2818], as specified in section 2.1. Set R318Enabled to false to disable this requirement.-->
    <Property name="R318Enabled" value="true"/>
    <!-- Set R55001Enabled to true to verify that the implementation does return MIME content for attachments of IPM.Note. When the IncludeMimeContent element is set to true in the AttachmentResponseShapeType complex type. Set R55001Enabled to false to disable this requirement.-->
    <Property name="R55001Enabled" value="false"/>
    <!-- Set R55002Enabled to true to verify that the implementation does return MIME content for attachments of IPM.Post. When the IncludeMimeContent element is set to true in the AttachmentResponseShapeType complex type. Set R55002Enabled to false to disable this requirement.-->
    <Property name="R55002Enabled" value="false"/>
    <!-- Set R55003Enabled to true to verify that the implementation does return MIME content for attachments of IPM.Appointment. When the IncludeMimeContent element is set to true in the AttachmentResponseShapeType complex type. Set R55003Enabled to false to disable this requirement.-->
    <Property name="R55003Enabled" value="false"/>
    <!-- Set R318011Enabled to true to verify that the implementation does introduce the ReferenceAttachmentType complex type. Set R318011Enabled to false to disable this requirement.-->
    <Property name="R318011Enabled" value="false"/>
    <!-- Set R318001Enabled to true to verify that the implementation does return ErrorInvalidItemForOperationCreateItemAttachment response code as specified in [MS-OXWSCDATA] section 2.2.5.24, when the item attachment is MeetingMessage, MeetingRequest, MeetingResponse or MeetingCancellation. Set R318001Enabled to false to disable this requirement.-->
    <Property name="R318001Enabled" value="true"/>
    <!-- Set R318014Enabled to true to verify that the implementation does return a MessageType item. Set R318014Enabled to false to disable this requirement.-->
    <Property name="R318014Enabled" value="true"/>
     <!-- Set R552Enabled to true to verify that the implementation does return ErrorUnsupportedMimeConversion response code when the IncludeMimeContent element is set to true and the attachment is not one of the accepted item classes [IPM.Note, IPM.Post, IPM.Appointment. Set R552Enabled to false to disable this requirement.-->
    <Property name="R552Enabled" value="true"/>
    <!-- Set R318003Enabled to true to verify that the implementation does suport IsInline element. Set R318003Enabled to false to disable this requirement.-->
    <Property name="R318003Enabled" value="false"/>
    <!-- Set R318005Enabled to true to verify that the implementation does suport IsContactPhoto element. Set R318005Enabled to false to disable this requirement.-->
    <Property name="R318005Enabled" value="false"/>
    <!-- Set R318009Enabled to true to verify that the implementation does support the Person element. Set R318009Enabled to false to disable this requirement.-->
    <Property name="R318009Enabled" value="false"/>
    <!-- Set R318013Enabled to true to verify that the implementation does support the FilterHtmlContent element. Set R318013Enabled to false to disable this requirement.-->
    <Property name="R318013Enabled" value="false"/>
    <!-- Set R351Enabled to true to verify that the implementation does support the CalendarItem element. Set R351Enabled to false to disable this requirement.-->
    <Property name="R351Enabled" value="true"/>
